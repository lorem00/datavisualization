{
  "name": "wootcloud",
  "version": "0.0.1",
  "description": "UI",
  "author": "wootcloud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:wootcloud/UI.git"
  },
  "engines": {
    "node": "6.4.0",
    "npm": "5.4.2"
  },
  "scripts": {
    "dev": "NODE_ENV=development node ./src/server",
    "start": "NODE_ENV=production node ./src/server/index.js",
    "prebuild:client": "mkdir -p ./build && rm -rf ./build/client",
    "build:client": "webpack -p --display-error-details --config=config/webpack.config.prod.js",
    "build:server": "mkdir -p ./build && rm -rf ./build/server && cp -r src/server build/server",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern node_modules -- .",
    "lint:watch": "node_modules/.bin/esw --ignore-path .gitignore --ignore-pattern node_modules config build build-dev --watch ",
    "test": "jest",
    "test:snapshots": "jest -u",
    "verify-tests": "jest --coverage",
    "mock": "mock-json-server src/resources/mock/db.json --port=3000"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(scss|css|jpg|png)$": "<rootDir>/src/client/empty-module.js"
    }
  },
  "postcss": {},
